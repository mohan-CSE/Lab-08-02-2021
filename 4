point = [0, 0]
 
if point[0]>0 and point[1]>0:
   print("First")
elif point[0]>0 and point[1]<0:
   print("Fourth")
elif point[0]<0 and point[1]>0:
   print("Second")
elif point[0]<0 and point[1]<0:
   print("Third")
elif point[0]==0 and point[1]>0:
   print("Y-axis")
elif (point[0]>0 or point[0]<0) and point[1]==0:
   print("X-axis")
else:
   print("Origin")
